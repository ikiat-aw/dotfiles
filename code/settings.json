{
  "security.workspace.trust.untrustedFiles": "open",
  "workbench.iconTheme": "file-icons-colourless",
  "editor.suggestSelection": "first",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "quokka.automaticRestart": true,
  "editor.formatOnSave": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.fontFamily": "PlemolJP-TextItalic",
  "editor.fontSize": 18,
  "scm.inputFontFamily": "editor",
  "scm.inputFontSize": 18,
  "editor.codeLensFontFamily": "PlemolJP-TextItalic",
  "markdown.preview.fontFamily": "PlemolJP-TextItalic",
  "debug.console.fontFamily": "PlemolJP-TextItalic",
  "terminal.integrated.fontSize": 14,
  "terminal.integrated.lineHeight": 1.5,
  "editor.lineHeight": 1.75,
  "window.zoomLevel": 1,
  "editor.cursorBlinking": "expand",
  "editor.renderLineHighlight": "gutter",
  "editor.wordWrap": "on",
  "editor.cursorSmoothCaretAnimation": true,
  "terminal.integrated.cursorStyle": "line",
  "terminal.integrated.fontFamily": "PlemolJPConsoleNF-MediumItalic",
  "terminal.integrated.cursorBlinking": true,
  "workbench.panel.opensMaximized": "always",
  "window.openFilesInNewWindow": "on",
  "window.openFoldersInNewWindow": "on",
  "workbench.colorTheme": "SynthWave '84",
  "vim.useSystemClipboard": true
}
